{
  "name": "expense-js",
  "version": "0.1.0",
  "description": "Expense Tracker",
  "repository": "https://github.com/francisl/expense-js",
  "main": "src/index.js",
  "scripts": {
    "start:default": "npm run build && cross-env NODE_ENV=default && node dist/index.js",
    "start": "npm run build && cross-env NODE_ENV=prod node dist/index.js",
    "start:watch": "supervisor --watch dist -- dist/index.js",
    "create:db": "node src/tools/db-init.js",
    "build": "babel src -d dist/",
    "test": "set NODE_ENV=test mocha -d --compilers js:babel-register",
    "test:oldschool": "npm run build && cross-env NODE_ENV=test node ./dist/test.js"
  },
  "author": "Francis Lavoie",
  "license": "GPL-3.0",
  "dependencies": {
    "del": "^3.0.0",
    "express": "^4.15.0",
    "extend": "^3.0.0",
    "lodash": "^4.0.0",
    "pug": "^2.0.0-beta6",
    "require-dot-file": "^0.4.0",
    "sqlite3": "^4.0.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "body-parser": "^1.15.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "sinon": "^7.2.3"
  }
}
