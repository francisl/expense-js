{
	"plugins": [
    "@babel/plugin-proposal-object-rest-spread", 
    "@babel/plugin-proposal-class-properties", 
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css"
    }]
  ],
	"presets": ["@babel/preset-react"],
  "sourceMaps": "both",
  "comments": false,
  "env": {
  	"production": {
    	"plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-runtime",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
      ]
    }
  }
}
