{
  "name": "expense-js",
  "version": "0.1.0",
  "description": "Expense Tracker",
  "repository": "https://github.com/francisl/expense-js",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node --presets=es2015 src/index.js",
    "start-dev": "supervisor --watch dist -- dist/index.js",
    "dev": "babel --presets=es2015 --watch src --out-dir dist",
    "build": "babel --presets=es2015 src -d dist/",
    "test:mocha": "mocha --compilers js:babel-register",
    "test": "NODE_ENV=test ava"
  },
  "ava": {
    "files": [
      "**/tests/*.js",
      "!node_modules/*"
    ],
    "failFast": true,
    "serial": true,
    "tap": true,
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "author": "Francis Lavoie",
  "license": "GPL-3.0",
  "dependencies": {
    "better-console": "^0.2.4",
    "del": "^1.2.1",
    "express": "^4.13.3",
    "extend": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-chmod": "^1.2.0",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-concat-css": "^2.2.0",
    "gulp-copy": "0.0.2",
    "gulp-flatten": "^0.1.1",
    "gulp-header": "^1.2.2",
    "gulp-help": "^1.6.0",
    "gulp-if": "^1.2.5",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-print": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rtlcss": "^0.1.4",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.4",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "map-stream": "0.0.6",
    "require-dot-file": "^0.4.0",
    "semantic-ui": "^2.1.4",
    "sqlite3": "^3.1.1",
    "yamljs": "^0.2.3"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "",
    "babel-core": "^6",
    "babel-preset-es2015": "^6.0.14",
    "babel-register": "^6.5.1",
    "mocha": "^2.4.5"
  }
}
